<div id="showArea">
    <table>
        <thead>
            <tr>
                <th>Title</th>
                <th>Author</th>
                <th>Description</th>
                <th>Year published</th>
            </tr>
        </thead>
        <tbody data-bind="foreach: books">
            <tr>
                <td data-bind="text: Title"></td>
                <td data-bind="text: Author"></td>
                <td data-bind="text: Description"></td>
                <td data-bind="text: PublishYear"></td>
            </tr>
        </tbody>
    </table>
</div>